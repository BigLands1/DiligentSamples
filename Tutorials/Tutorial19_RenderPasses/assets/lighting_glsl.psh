layout(input_attachment_index = 0, binding = 0) uniform subpassInput in_Color;
layout(input_attachment_index = 1, binding = 1) uniform subpassInput in_DepthZ;

layout(location = 0) out vec4 out_Color;

void main()
{
    out_Color.rgb = subpassLoad(in_Color).bgr;
    out_Color.a = subpassLoad(in_DepthZ).x;
}
